{
  "expo": {
    "name": "Ecoleta",
    "slug": "Ecoleta",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "packagerOpts": {
      "sourceExts": ["js", "ts", "tsx", "jsx"]
  },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.CompanyName.AppName",
      "versionCode": 1,
      "config": {
        "googleMaps": {
          "apiKey": "Your api Key"
        }
      }

    }
  }
}